<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from NortekServiceService WSDL service -->

  <service name="NortekService" provider="java:RPC" style="rpc" use="encoded">
      <parameter name="wsdlTargetNamespace" value="urn:nortek"/>
      <parameter name="wsdlServiceElement" value="NortekServiceService"/>
      <parameter name="wsdlServicePort" value="NortekService"/>
      <parameter name="className" value="is.idega.idegaweb.campus.webservice.nortek.server.NortekServiceSoapBindingSkeleton"/>
      <parameter name="wsdlPortType" value="NortekService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <parameter name="allowedMethods" value="*"/>
      <parameter name="scope" value="Session"/>

  </service>
</deployment>
